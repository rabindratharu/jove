{
  "name": "acf/share",
  "apiVersion": "2",
  "title": "Jove: Social Share Block",
  "description": "A custom social share block with popup.",
  "category": "formatting",
  "icon": "format-gallery",
  "keywords": [
    "social",
    "popup",
    "share"
  ],
  "acf": {
    "mode": "preview",
    "renderTemplate": "render.php"
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "wide"
    }
  },
  "supports": {
    "align": [
      "full",
      "wide"
    ],
    "anchor": true,
    "className": true,
    "color": {
      "background": true
    },
    "mode": false
  },
  "style": [
    "popup-css"
  ],
  "viewScript": [
    "file:./../../js/share.js"
  ]
}