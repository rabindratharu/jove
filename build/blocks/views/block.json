{
  "name": "acf/views",
  "apiVersion": "2",
  "title": "Jove: Views Block",
  "description": "A custom views block.",
  "category": "formatting",
  "icon": "format-gallery",
  "keywords": [
    "views",
    "count",
    "post"
  ],
  "acf": {
    "mode": "preview",
    "renderTemplate": "render.php"
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "wide"
    }
  },
  "supports": {
    "align": [
      "full",
      "wide"
    ],
    "anchor": true,
    "className": true,
    "color": {
      "background": true
    },
    "mode": false
  },
  "style": [
    "file:./../../css/notice.css"
  ],
  "viewScript": [
    "file:./../../js/notice.js"
  ]
}