{
	"name": "acf/cta",
	"apiVersion": "2",
	"title": "Jove: CTA Block",
	"description": "A custom call to action block with popup video.",
	"category": "formatting",
	"icon": "format-gallery",
	"keywords": ["cta", "popup", "video"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "render.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "wide"
		}
	},
	"supports": {
		"align": ["wide", "full"]
	},
	"style": ["file:./../../css/cta.css"],
	"viewScript": ["file:./../../js/cta.js"]
}
