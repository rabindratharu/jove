{
	"name": "acf/video",
	"apiVersion": "2",
	"title": "Jove: Video Block",
	"description": "A custom views block.",
	"category": "formatting",
	"icon": "format-gallery",
	"keywords": ["details", "video", "post"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "render.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "wide"
		}
	},
	"supports": {
		"align": ["full", "wide"],
		"anchor": true,
		"className": true,
		"color": {
			"background": true
		},
		"mode": false
	},
	"style": ["file:./../../css/video.css"]
}
